<UserControl x:Class="OmoSeitokuEreceipt.ER.Controls.SisikiFilterControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" FontStretch="ExtraExpanded"
             d:DesignHeight="120" d:DesignWidth="400">
    <UserControl.Resources>
        <ControlTemplate x:Key="ButtonUncheckedBackground" TargetType="ToggleButton">
            <Border CornerRadius="3" Background="#FFCEEDF7"
                    BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                <Viewbox>
                    <ContentPresenter/>
                </Viewbox>
            </Border>
        </ControlTemplate>
        <ControlTemplate x:Key="ButtonCheckedBackground" TargetType="ToggleButton">
            <Border CornerRadius="3" Background="#FFFFCD63"
                    BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                <Viewbox>
                    <ContentPresenter/>
                </Viewbox>
            </Border>
        </ControlTemplate>
        <Style TargetType="ToggleButton">
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Template" Value="{StaticResource ButtonUncheckedBackground}"/>
            <Style.Triggers>
                <Trigger Property="IsChecked" Value="True">
                    <Setter Property="Template" Value="{StaticResource ButtonCheckedBackground}"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition Width="24"/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition Height="12"/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <ToggleButton x:Name="_btn1055" Content="E" Grid.Column="3" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1054" Content="D" Grid.Column="4" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1053" Content="C" Grid.Column="5" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1052" Content="B" Grid.Column="6" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1051" Content="A" Grid.Column="7" Grid.Row="0"/>

        <ToggleButton x:Name="_btn1061" Content="A" Grid.Column="9" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1062" Content="B" Grid.Column="10" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1063" Content="C" Grid.Column="11" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1064" Content="D" Grid.Column="12" Grid.Row="0"/>
        <ToggleButton x:Name="_btn1065" Content="E" Grid.Column="13" Grid.Row="0"/>

        <ToggleButton x:Name="_btnMultiSelect" Grid.Column="14" Grid.ColumnSpan="3" Grid.Row="0" Margin="8 4 2 2" Background="LightBlue">
