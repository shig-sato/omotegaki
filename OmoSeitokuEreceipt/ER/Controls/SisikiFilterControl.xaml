            </Viewbox>
        </ToggleButton>

        <ToggleButton x:Name="_btn1018" Content="8" Grid.Column="0" Grid.Row="1"/>
        <ToggleButton x:Name="_btn1017" Content="7" Grid.Column="1" Grid.Row="1"/>
        <ToggleButton x:Name="_btn1016" Content="6" Grid.Column="2" Grid.Row="1"/>
        <ToggleButton x:Name="_btn1015" Content="5" Grid.Column="3" Grid.Row="1"/>
        <ToggleButton x:Name="_btn1014" Content="4" Grid.Column="4" Grid.Row="1"/>
